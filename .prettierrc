{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/store/(.*)$",
    "^@/hooks/(.*)$",
    "^@/page/(.*)$",
    "^@/utils/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$",
    "(.css)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
